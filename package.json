{
  "name": "@xwtec/cli",
  "version": "0.0.6",
  "description": "xwtec work cli",
  "main": "lib/index.js",
  "repository": "https://github.com/xwtec/cli",
  "author": "fisker",
  "bin": {
    "xwtec": "bin/index.js",
    "xw": "bin/index.js"
  },
  "license": "MIT",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm-run-all --parallel build:*",
    "build:js": "rollup --config",
    "lint": "eslint **/*.js --fix",
    "dist": "npm-run-all --parallel dist:*",
    "dist:npm": "np --no-yarn --no-cleanup",
    "release": "npm-run-all --sequential lint clean build dist"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cowsay": "^1.4.0",
    "inquirer": "^6.2.2",
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@xwtec/eslint-config": "^3.5.3",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.14.1",
    "lint-staged": "^8.1.4",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
